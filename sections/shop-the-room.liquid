    <h2 class="fade-in-up" data-aos data-aos-delay="100">{{ section.settings.title }}</h2>
    <div class="shop-the-room__tabs-desktop hide-mobile fade-in-up" data-aos data-aos-delay="200">
      {% for block in section.blocks %}
        <button class="btn{% if forloop.first %} active{% endif %}" data-tab="{{ forloop.index0 }}">{{ block.settings.title }}</button>
      {% endfor %}
    </div>
    <div class="shop-the-room__tabs swiper hide-desktop fade-in-up" data-aos data-aos-delay="200">
      <div class="swiper-wrapper">
        {% for block in section.blocks %}
          <button class="swiper-slide btn"><span>{{ block.settings.title }}</span></button>
        {% endfor %}
      </div>
    </div>
