  <div class="customer-body-content">
    <div class="customer-container">
      <div class="account-box desktop-account">
        <div class="account-left">
          <ul class="account-links">
            <li><a class="order-link cta account-button active">Order history</a></li>
            <li><a class="address-link cta account-button">Billing Information</a></li>
            <li>
              <a href="{{ routes.account_logout_url }}" class="cta">
                {{ 'customer.log_out' | t }}
              </a>
            </li>
          </ul>
        </div>
        <div class="account-right">
          <div class="account-section-item address-item ">
            <h2 class="title sh2">Billing Information</h2>
            {% comment %}
            <h3 class="sh1">Shipping Information</h3>
            {% endcomment %}
            {{ customer.default_address | format_address }}
            <a class="view-btn cta" href="{{ routes.account_addresses_url }}">
              {{ 'customer.account.view_addresses' | t }} ({{ customer.addresses_count }})
            </a>
          </div>  
          <div class="account-section-item order-item open">
            <h2 class="title sh2">Order History</h2>
            {% comment %}
            <h3>{{ 'customer.orders.title' | t }}</h3>
            {% endcomment %}
            {% paginate customer.orders by 20 %}
            {%- if customer.orders.size > 0 -%}
            <table role="table" class="order-history body-2">
              <caption class="visually-hidden">{{ 'customer.orders.title' | t }}</caption>
              <thead role="rowgroup">
                <tr role="row">
                  <th class="nav links" id="ColumnOrder" scope="col" role="columnheader">{{ 'customer.orders.order_number' | t }}</th>
                  <th class="nav links" id="ColumnDate" scope="col" role="columnheader">{{ 'customer.orders.date' | t }}</th>
                  <th class="nav links visually-hidden" id="ColumnPayment" scope="col" role="columnheader">{{ 'customer.orders.payment_status' | t }}</th>
                  <th class="nav links" id="ColumnFulfillment" scope="col" role="columnheader">{{ 'customer.orders.fulfillment_status' | t }}</th>
                  <th class="nav links visually-hidden"id="ColumnTotal" scope="col" role="columnheader">{{ 'customer.orders.total' | t }}</th>
                </tr>
              </thead>
              <tbody role="rowgroup">
                {%- for order in customer.orders -%}
                <tr role="row">
                  <td
                      id="RowOrder"
                      role="cell"
                      headers="ColumnOrder"
                      data-label="{{ 'customer.orders.order_number' | t }}"
                      class="body-2"
                      >
                    <a class="body-2" href="{{ order.customer_url }}" aria-label="{{ 'customer.orders.order_number_link' | t: number: order.name }}">
                      {{ order.name }}
                    </a>
                  </td>
                  <td class="body-2" headers="RowOrder ColumnDate" role="cell" data-label="{{ 'customer.orders.date' | t }}">
                    {{ order.created_at | time_tag: format: 'date' }}
                  </td>
                  <td class="body-2 visually-hidden" headers="RowOrder ColumnPayment" role="cell" data-label="{{ 'customer.orders.payment_status' | t }}">
                    {{ order.financial_status_label }}
                  </td>
                  <td class="body-2" headers="RowOrder ColumnFulfillment" role="cell" data-label="{{ 'customer.orders.fulfillment_status' | t }}">
                    {{ order.fulfillment_status_label }}
                  </td>
                  <td class="body-2 visually-hidden" headers="RowOrder ColumnTotal" role="cell" data-label="{{ 'customer.orders.total' | t }}">
                    {{ order.total_price | money_with_currency }}
                  </td>
                  <td class="body-2 view-order" headers="RowOrder ColumnDate" role="cell" data-label="{{ 'customer.orders.date' | t }}" class="order_date">
                    <a href="{{ order.customer_url }}" aria-label="{{ 'customer.orders.order_number_link' | t: number: order.name }}" class="view_order">
                      View Order
                    </a>
                  </td>
                </tr>
                {%- endfor -%}
              </tbody>
            </table>
            {%- else -%}
            <p>{{ 'customer.orders.none' | t }}</p>
            {%- endif -%}
            {%- if paginate.pages > 1 -%}
            {%- if paginate.parts.size > 0 -%}
            <nav class="pagination" role="navigation" aria-label="{{ 'general.pagination.label' | t }}">
              <ul role="list">
                {%- if paginate.previous -%}
                <li>
                  <a href="{{ paginate.previous.url }}" aria-label="{{ 'general.pagination.previous' | t }}">
                    <svg aria-hidden="true" focusable="false" viewBox="0 0 10 6">
                      <path fill-rule="evenodd" clip-rule="evenodd" d="M9.354.646a.5.5 0 00-.708 0L5 4.293 1.354.646a.5.5 0 00-.708.708l4 4a.5.5 0 00.708 0l4-4a.5.5 0 000-.708z" fill="currentColor">
                        </svg>
                      </a>
                    </li>
                  {%- endif -%}

                  {%- for part in paginate.parts -%}
                <li>
                  {%- if part.is_link -%}
                  <a href="{{ part.url }}" aria-label="{{ 'general.pagination.page' | t: number: part.title }}">{{ part.title }}</a>
                  {%- else -%}
                  {%- if part.title == paginate.current_page -%}
                  <span aria-current="page" aria-label="{{ 'general.pagination.page' | t: number: part.title }}">{{ part.title }}</span>
                  {%- else -%}
                  <span>{{ part.title }}</span>
                  {%- endif -%}
                  {%- endif -%}
                </li>
                {%- endfor -%}

                {%- if paginate.next -%}
                <li>
                  <a href="{{ paginate.next.url }}" aria-label="{{ 'general.pagination.next' | t }}" >
                    <svg aria-hidden="true" focusable="false" viewBox="0 0 10 6">
                      <path fill-rule="evenodd" clip-rule="evenodd" d="M9.354.646a.5.5 0 00-.708 0L5 4.293 1.354.646a.5.5 0 00-.708.708l4 4a.5.5 0 00.708 0l4-4a.5.5 0 000-.708z" fill="currentColor">
                        </svg>
                      </a>
                    </li>
                  {%- endif -%}
              </ul>
            </nav>
            {%- endif -%}
            {%- endif -%}
            {% endpaginate %}
          </div>
          <div class="account-section-item subscription-item">
            <h2 class="title">My Subscription</h2>
          </div>
        </div>
      </div>

      <div class="account-box mobile-account">
        <div class="account-left">
          <ul class="account-links list-unstyled">
            <li class="init clearfix"><a class="order-link cta account-button active" href="javascript:void(0)">Order history</a></li>
