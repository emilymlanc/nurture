      </div>
    </div>
  </div>
  
</div>
<script type="text/javascript">
  $(document).ready(function(){
    $('.address-link').click(function(){
      $('.account-links a').removeClass('active');
      $(this).addClass('active');
      $('.address-item').addClass('open');
      $('.order-item').removeClass('open');
      $('.subscription-item').removeClass('open');
    });
    
    $('.order-link').click(function(){
      $('.account-links a').removeClass('active');
      $(this).addClass('active');
      $('.order-item').addClass('open');
      $('.address-item').removeClass('open');
      $('.subscription-item').removeClass('open');
    });
    
    $("ul").on("click", ".init", function() {
      $(this).closest("ul").children('li:not(.init)').toggle();
    });

